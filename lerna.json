{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "npmClient": "npm",
      "allowBranch": [
        "main",
        "develope"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "version": "2.0.0",
  "npmClient": "npm",
  "useWorkspaces": true
}
